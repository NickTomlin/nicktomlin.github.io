{
  "name": "nick-tomlin.com",
  "version": "1.0.0",
  "description": "My home page. Right now it's ruby and JS just like the things I build.",
  "private": true,
  "main": "index.js",
  "scripts": {
    "start": "concurrently 'npm run dev:jekyll' 'npm run dev:assets'",
    "build": "npm run build:webpack && npm run build:jekyll",
    "build:jekyll": "bundle exec jekyll build",
    "build:webpack": "webpack --mode production",
    "dev:jekyll": "bundle exec jekyll serve",
    "dev:assets": "webpack --watch",
    "prettier": "prettier --write javascript/**/*.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/NickTomlin/nicktomlin.github.io.git"
  },
  "author": "",
  "bugs": {
    "url": "https://github.com/NickTomlin/nicktomlin.github.io/issues"
  },
  "homepage": "https://github.com/NickTomlin/nicktomlin.github.io#readme",
  "dependencies": {
    "apollo-boost": "^0.1.7",
    "apollo-client": "^2.3.2",
    "babel": "^6.23.0",
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.4",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "concurrently": "^3.5.1",
    "gh-pages": "^1.2.0",
    "glob": "^7.1.2",
    "graphql": "^0.13.2",
    "graphql-tag": "^2.9.2",
    "prettier": "^1.13.3",
    "react": "^16.4.0",
    "react-apollo": "^2.1.4",
    "react-dom": "^16.4.2",
    "webpack": "^4.10.2",
    "webpack-command": "^0.2.0"
  }
}
